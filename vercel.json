{
  "version": 2,
  "builds": [
    { "src": "package.json", "use": "@vercel/static-build" },
    { "src": "server/dist/server.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/site.webmanifest", "dest": "/site.webmanifest" },
    { "src": "/assets/(.*)", "dest": "/assets/$1" },
    { "src": "/(.*\\.js)", "dest": "/$1" },
    { "src": "/(.*\\.css)", "dest": "/$1" },
    { "src": "/(.*\\.png)", "dest": "/$1" },
    { "src": "/(.*\\.ico)", "dest": "/$1" },
    { "src": "/(.*\\.svg)", "dest": "/$1" },
    { "src": "/(.*\\.jpg)", "dest": "/$1" },
    { "src": "/(.*\\.jpeg)", "dest": "/$1" },
    { "src": "/(.*\\.webp)", "dest": "/$1" },
    { "src": "/(.*\\.woff2?)", "dest": "/$1" },
    { "src": "/(.*\\.ttf)", "dest": "/$1" },
    { "src": "/(.*\\.map)", "dest": "/$1" },
    { "src": "/(.*\\.pdf)", "dest": "/$1" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}